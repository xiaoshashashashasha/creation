{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nimport { ElMessage } from \"element-plus\";\nimport { hairstyleList, hairstyleInfo, hairstyleDel, hairstyleUpdate, hairstyleAdd } from \"@/api/hairstyle\";\nimport { uploadFile } from \"@/api/fileUpload\";\nimport '@wangeditor/editor/dist/css/style.css';\nimport { Editor, Toolbar } from '@wangeditor/editor-for-vue';\nimport { onBeforeUnmount } from 'vue';\nexport default {\n  __name: 'HairstyleManage',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const showEditEditor = ref(false);\n    const showCreateEditor = ref(false);\n\n    // 编辑器状态\n    let editor1 = null;\n    let editor2 = null;\n    const editEditorKey = ref(0);\n    const createEditorKey = ref(0);\n    const handleEditorChange1 = editor => {\n      editForm.value.content = editor.getHtml();\n    };\n    const handleEditorChange2 = editor => {\n      createForm.value.content = editor.getHtml();\n    };\n\n    // 清理\n    onBeforeUnmount(() => {\n      if (editor1) editor1.destroy();\n      if (editor2) editor2.destroy();\n    });\n    const editorConfig = {\n      placeholder: '请输入内容...',\n      MENU_CONF: {\n        uploadImage: {\n          async customUpload(file, insertFn) {\n            try {\n              const res = await uploadFile(file);\n              const url = res.data;\n              insertFn(String(url), '', ''); // 插入图片\n              ElMessage.success(\"图片上传成功\");\n            } catch (err) {\n              ElMessage.error(\"图片上传失败\");\n            }\n          }\n        }\n      },\n      // ✅ 开启图片工具栏（调整宽高、替换、删除等）\n      EXTEND_CONF: {\n        image: {\n          width: 'auto',\n          // 默认值\n          height: 'auto',\n          isResizable: true // ✨这个是关键，允许拖拽尺寸\n        }\n      }\n    };\n    const closeEditDialog = () => {\n      editDialogVisible.value = false;\n      editor1?.destroy();\n      editor1 = null;\n      showEditEditor.value = false;\n      editEditorKey.value++; // 强制刷新 toolbar\n    };\n    const closeCreateDialog = () => {\n      createDialogVisible.value = false;\n      editor2?.destroy();\n      editor2 = null;\n      showCreateEditor.value = false;\n      createEditorKey.value++;\n    };\n    const tableData = ref([]);\n    const pageNum = ref(1);\n    const pageSize = ref(6);\n    const total = ref(0);\n    const dialogVisible = ref(false);\n    const editDialogVisible = ref(false);\n    const detail = ref(null);\n    const editForm = ref({});\n    const keyWord = ref(\"\");\n    const createDialogVisible = ref(false);\n    const createForm = ref({\n      hairstyle_name: \"\",\n      hairstyle_pic: \"\",\n      content: \"\"\n    });\n    const fetchData = async () => {\n      try {\n        const res = await hairstyleList({\n          pageNum: pageNum.value,\n          pageSize: pageSize.value,\n          keyWord: keyWord.value\n        });\n        tableData.value = res.data.items;\n        total.value = res.data.total;\n      } catch (err) {\n        ElMessage.error(err.msg || \"获取发型数据失败\");\n      }\n    };\n    const handleSearch = () => {\n      pageNum.value = 1;\n      fetchData();\n    };\n    const handleReset = () => {\n      keyWord.value = \"\";\n      fetchData();\n    };\n    const handlePageChange = newPage => {\n      pageNum.value = newPage;\n      fetchData();\n    };\n    const openDetailDialog = async id => {\n      try {\n        const res = await hairstyleInfo(id);\n        detail.value = res.data;\n        dialogVisible.value = true;\n      } catch (err) {\n        ElMessage.error(err.msg || \"获取发型详情失败\");\n      }\n    };\n    const openEditDialog = async id => {\n      try {\n        const res = await hairstyleInfo(id);\n        editForm.value = {\n          ...res.data\n        };\n        editDialogVisible.value = true;\n        // 延迟挂载编辑器\n        setTimeout(() => {\n          showEditEditor.value = true;\n        }, 500);\n      } catch (err) {\n        ElMessage.error(err.msg || \"获取发型详情失败\");\n      }\n    };\n    const handleDelete = async id => {\n      try {\n        await hairstyleDel(id);\n        ElMessage.success(\"删除成功\");\n        fetchData();\n      } catch (err) {\n        ElMessage.error(err.msg || \"删除失败\");\n      }\n    };\n    const handleCustomUpload = async ({\n      file\n    }) => {\n      try {\n        const res = await uploadFile(file);\n        editForm.value.hairstyle_pic = res.data;\n        ElMessage.success(\"上传成功\");\n      } catch (err) {\n        ElMessage.error(\"上传失败\");\n      }\n    };\n    const handleCreateUpload = async ({\n      file\n    }) => {\n      try {\n        const res = await uploadFile(file);\n        createForm.value.hairstyle_pic = res.data;\n        ElMessage.success(\"上传成功\");\n      } catch (err) {\n        ElMessage.error(\"上传失败\");\n      }\n    };\n    const handleSaveEdit = async () => {\n      try {\n        await hairstyleUpdate(editForm.value);\n        ElMessage.success(\"保存成功\");\n        editDialogVisible.value = false;\n        fetchData();\n      } catch (err) {\n        ElMessage.error(err.msg || \"保存失败\");\n      }\n    };\n    const openCreateDialog = () => {\n      createForm.value = {\n        hairstyle_name: \"\",\n        hairstyle_pic: \"\",\n        content: \"\"\n      };\n      createDialogVisible.value = true;\n      setTimeout(() => {\n        showCreateEditor.value = true;\n      }, 100);\n    };\n    const handleCreateSubmit = async () => {\n      if (!createForm.value.hairstyle_name) {\n        return ElMessage.warning(\"请输入发型名称\");\n      }\n      try {\n        await hairstyleAdd(createForm.value);\n        ElMessage.success(\"新建成功\");\n        createDialogVisible.value = false;\n        fetchData();\n      } catch (err) {\n        ElMessage.error(err.msg || \"新建失败\");\n      }\n    };\n    onMounted(() => {\n      fetchData();\n    });\n    const __returned__ = {\n      showEditEditor,\n      showCreateEditor,\n      get editor1() {\n        return editor1;\n      },\n      set editor1(v) {\n        editor1 = v;\n      },\n      get editor2() {\n        return editor2;\n      },\n      set editor2(v) {\n        editor2 = v;\n      },\n      editEditorKey,\n      createEditorKey,\n      handleEditorChange1,\n      handleEditorChange2,\n      editorConfig,\n      closeEditDialog,\n      closeCreateDialog,\n      tableData,\n      pageNum,\n      pageSize,\n      total,\n      dialogVisible,\n      editDialogVisible,\n      detail,\n      editForm,\n      keyWord,\n      createDialogVisible,\n      createForm,\n      fetchData,\n      handleSearch,\n      handleReset,\n      handlePageChange,\n      openDetailDialog,\n      openEditDialog,\n      handleDelete,\n      handleCustomUpload,\n      handleCreateUpload,\n      handleSaveEdit,\n      openCreateDialog,\n      handleCreateSubmit,\n      ref,\n      onMounted,\n      get ElMessage() {\n        return ElMessage;\n      },\n      get hairstyleList() {\n        return hairstyleList;\n      },\n      get hairstyleInfo() {\n        return hairstyleInfo;\n      },\n      get hairstyleDel() {\n        return hairstyleDel;\n      },\n      get hairstyleUpdate() {\n        return hairstyleUpdate;\n      },\n      get hairstyleAdd() {\n        return hairstyleAdd;\n      },\n      get uploadFile() {\n        return uploadFile;\n      },\n      get Editor() {\n        return Editor;\n      },\n      get Toolbar() {\n        return Toolbar;\n      },\n      onBeforeUnmount\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","ElMessage","hairstyleList","hairstyleInfo","hairstyleDel","hairstyleUpdate","hairstyleAdd","uploadFile","Editor","Toolbar","onBeforeUnmount","showEditEditor","showCreateEditor","editor1","editor2","editEditorKey","createEditorKey","handleEditorChange1","editor","editForm","value","content","getHtml","handleEditorChange2","createForm","destroy","editorConfig","placeholder","MENU_CONF","uploadImage","customUpload","file","insertFn","res","url","data","String","success","err","error","EXTEND_CONF","image","width","height","isResizable","closeEditDialog","editDialogVisible","closeCreateDialog","createDialogVisible","tableData","pageNum","pageSize","total","dialogVisible","detail","keyWord","hairstyle_name","hairstyle_pic","fetchData","items","msg","handleSearch","handleReset","handlePageChange","newPage","openDetailDialog","id","openEditDialog","setTimeout","handleDelete","handleCustomUpload","handleCreateUpload","handleSaveEdit","openCreateDialog","handleCreateSubmit","warning"],"sources":["C:/Users/32561/Desktop/毕设仓库/beauty-front/src/views/manage/hairstyle/HairstyleManage.vue"],"sourcesContent":["<script setup>\r\nimport {ref, onMounted} from \"vue\"\r\nimport {ElMessage} from \"element-plus\"\r\nimport {\r\n  hairstyleList,\r\n  hairstyleInfo,\r\n  hairstyleDel,\r\n  hairstyleUpdate,\r\n  hairstyleAdd\r\n} from \"@/api/hairstyle\"\r\nimport {uploadFile} from \"@/api/fileUpload\"\r\n\r\nimport '@wangeditor/editor/dist/css/style.css'\r\nimport { Editor, Toolbar } from '@wangeditor/editor-for-vue'\r\nimport { onBeforeUnmount } from 'vue'\r\n\r\nconst showEditEditor = ref(false)\r\nconst showCreateEditor = ref(false)\r\n\r\n\r\n// 编辑器状态\r\nlet editor1 = null\r\nlet editor2 = null\r\nconst editEditorKey = ref(0)\r\nconst createEditorKey = ref(0)\r\n\r\n\r\nconst handleEditorChange1 = (editor) => {\r\n  editForm.value.content = editor.getHtml()\r\n}\r\n\r\nconst handleEditorChange2 = (editor) => {\r\n  createForm.value.content = editor.getHtml()\r\n}\r\n\r\n// 清理\r\nonBeforeUnmount(() => {\r\n  if (editor1) editor1.destroy()\r\n  if (editor2) editor2.destroy()\r\n})\r\n\r\n\r\nconst editorConfig = {\r\n  placeholder: '请输入内容...',\r\n  MENU_CONF: {\r\n    uploadImage: {\r\n      async customUpload(file, insertFn) {\r\n        try {\r\n          const res = await uploadFile(file)\r\n          const url = res.data\r\n          insertFn(String(url), '', '') // 插入图片\r\n          ElMessage.success(\"图片上传成功\")\r\n        } catch (err) {\r\n          ElMessage.error(\"图片上传失败\")\r\n        }\r\n      }\r\n    }\r\n  },\r\n  // ✅ 开启图片工具栏（调整宽高、替换、删除等）\r\n  EXTEND_CONF: {\r\n    image: {\r\n      width: 'auto', // 默认值\r\n      height: 'auto',\r\n      isResizable: true // ✨这个是关键，允许拖拽尺寸\r\n    }\r\n  }\r\n}\r\n\r\nconst closeEditDialog = () => {\r\n  editDialogVisible.value = false\r\n  editor1?.destroy()\r\n  editor1 = null\r\n  showEditEditor.value = false\r\n  editEditorKey.value++ // 强制刷新 toolbar\r\n}\r\n\r\nconst closeCreateDialog = () => {\r\n  createDialogVisible.value = false\r\n  editor2?.destroy()\r\n  editor2 = null\r\n  showCreateEditor.value = false\r\n  createEditorKey.value++\r\n}\r\n\r\n\r\n\r\n\r\nconst tableData = ref([])\r\nconst pageNum = ref(1)\r\nconst pageSize = ref(6)\r\nconst total = ref(0)\r\nconst dialogVisible = ref(false)\r\nconst editDialogVisible = ref(false)\r\nconst detail = ref(null)\r\nconst editForm = ref({})\r\nconst keyWord = ref(\"\")\r\n\r\n\r\nconst createDialogVisible = ref(false)\r\nconst createForm = ref({\r\n  hairstyle_name: \"\",\r\n  hairstyle_pic: \"\",\r\n  content: \"\"\r\n})\r\n\r\n\r\n\r\n\r\nconst fetchData = async () => {\r\n  try {\r\n    const res = await hairstyleList({\r\n      pageNum: pageNum.value,\r\n      pageSize: pageSize.value,\r\n      keyWord: keyWord.value\r\n    })\r\n    tableData.value = res.data.items\r\n    total.value = res.data.total\r\n  } catch (err) {\r\n    ElMessage.error(err.msg || \"获取发型数据失败\")\r\n  }\r\n}\r\n\r\nconst handleSearch = () => {\r\n  pageNum.value = 1\r\n  fetchData()\r\n}\r\n\r\nconst handleReset = () => {\r\n  keyWord.value = \"\"\r\n  fetchData()\r\n}\r\n\r\nconst handlePageChange = (newPage) => {\r\n  pageNum.value = newPage\r\n  fetchData()\r\n}\r\n\r\nconst openDetailDialog = async (id) => {\r\n  try {\r\n    const res = await hairstyleInfo(id)\r\n    detail.value = res.data\r\n    dialogVisible.value = true\r\n  } catch (err) {\r\n    ElMessage.error(err.msg || \"获取发型详情失败\")\r\n  }\r\n}\r\n\r\nconst openEditDialog = async (id) => {\r\n  try {\r\n    const res = await hairstyleInfo(id)\r\n    editForm.value = { ...res.data }\r\n    editDialogVisible.value = true\r\n    // 延迟挂载编辑器\r\n    setTimeout(() => {\r\n      showEditEditor.value = true\r\n    }, 500)\r\n  } catch (err) {\r\n    ElMessage.error(err.msg || \"获取发型详情失败\")\r\n  }\r\n}\r\n\r\nconst handleDelete = async (id) => {\r\n  try {\r\n    await hairstyleDel(id)\r\n    ElMessage.success(\"删除成功\")\r\n    fetchData()\r\n  } catch (err) {\r\n    ElMessage.error(err.msg || \"删除失败\")\r\n  }\r\n}\r\n\r\nconst handleCustomUpload = async ({file}) => {\r\n  try {\r\n    const res = await uploadFile(file)\r\n    editForm.value.hairstyle_pic = res.data\r\n    ElMessage.success(\"上传成功\")\r\n  } catch (err) {\r\n    ElMessage.error(\"上传失败\")\r\n  }\r\n}\r\n\r\nconst handleCreateUpload = async ({file}) => {\r\n  try {\r\n    const res = await uploadFile(file)\r\n    createForm.value.hairstyle_pic = res.data\r\n    ElMessage.success(\"上传成功\")\r\n  } catch (err) {\r\n    ElMessage.error(\"上传失败\")\r\n  }\r\n}\r\n\r\nconst handleSaveEdit = async () => {\r\n  try {\r\n    await hairstyleUpdate(editForm.value)\r\n    ElMessage.success(\"保存成功\")\r\n    editDialogVisible.value = false\r\n    fetchData()\r\n  } catch (err) {\r\n    ElMessage.error(err.msg || \"保存失败\")\r\n  }\r\n}\r\n\r\nconst openCreateDialog = () => {\r\n  createForm.value = {\r\n    hairstyle_name: \"\",\r\n    hairstyle_pic: \"\",\r\n    content: \"\"\r\n  }\r\n  createDialogVisible.value = true\r\n  setTimeout(() => {\r\n    showCreateEditor.value = true\r\n  }, 100)\r\n}\r\nconst handleCreateSubmit = async () => {\r\n  if (!createForm.value.hairstyle_name) {\r\n    return ElMessage.warning(\"请输入发型名称\")\r\n  }\r\n  try {\r\n    await hairstyleAdd(createForm.value)\r\n    ElMessage.success(\"新建成功\")\r\n    createDialogVisible.value = false\r\n    fetchData()\r\n  } catch (err) {\r\n    ElMessage.error(err.msg || \"新建失败\")\r\n  }\r\n}\r\n\r\nonMounted(() => {\r\n  fetchData()\r\n})\r\n</script>\r\n\r\n<template>\r\n  <el-row :gutter=\"0\">\r\n    <div class=\"path\">\r\n      <p style=\"font-size: 20px; margin-top: 5px\">发型管理</p>\r\n    </div>\r\n\r\n    <div class=\"search\">\r\n      <el-row justify=\"space-between\" align=\"middle\" style=\"padding: 10px\">\r\n        <el-col :span=\"20\">\r\n          <el-row :gutter=\"10\">\r\n            <el-col :span=\"6\">\r\n              <el-input v-model=\"keyWord\" placeholder=\"请输入发型名称\" clearable @keyup.enter=\"handleSearch\"/>\r\n            </el-col>\r\n            <el-col :span=\"6\">\r\n              <el-button type=\"primary\" @click=\"handleSearch\">搜索</el-button>\r\n              <el-button @click=\"handleReset\">重置</el-button>\r\n            </el-col>\r\n          </el-row>\r\n        </el-col>\r\n        <el-col :span=\"4\" style=\"text-align: right\">\r\n          <el-button type=\"primary\" @click=\"openCreateDialog\">新建发型</el-button>\r\n        </el-col>\r\n      </el-row>\r\n    </div>\r\n\r\n    <el-table :data=\"tableData\" style=\"width: 100%\">\r\n      <el-table-column prop=\"hairstyle_id\" label=\"发型ID\" width=\"120px\"/>\r\n      <el-table-column label=\"封面\" width=\"200px\">\r\n        <template #default=\"{ row }\">\r\n          <img :src=\"row.hairstyle_pic\" style=\"width: 200px; height: 100px; object-fit: cover\"/>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"hairstyle_name\" label=\"发型名\" width=\"160px\"/>\r\n      <el-table-column prop=\"created_at\" label=\"创建时间\" width=\"240px\"/>\r\n      <el-table-column prop=\"updated_at\" label=\"更新时间\" width=\"240px\"/>\r\n      <el-table-column label=\"操作\" width=\"360px\">\r\n        <template #default=\"{ row }\">\r\n          <el-button size=\"small\" @click=\"openDetailDialog(row.hairstyle_id)\">查看</el-button>\r\n          <el-button size=\"small\" @click=\"openEditDialog(row.hairstyle_id)\">编辑</el-button>\r\n          <el-button size=\"small\" type=\"danger\" @click=\"handleDelete(row.hairstyle_id)\">删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <div style=\"text-align: center; margin-top: 20px\">\r\n      <el-pagination\r\n          background\r\n          layout=\"prev, pager, next, ->, total\"\r\n          :total=\"total\"\r\n          :current-page=\"pageNum\"\r\n          :page-size=\"pageSize\"\r\n          @current-change=\"handlePageChange\"\r\n      />\r\n    </div>\r\n\r\n    <!-- 查看弹窗 -->\r\n    <el-dialog v-model=\"dialogVisible\" title=\"发型详情\" width=\"40%\">\r\n      <div v-if=\"detail\">\r\n        <h3>{{ detail.hairstyle_name }}</h3>\r\n        <img :src=\"detail.hairstyle_pic\" style=\"width: 100%; max-height: 300px; object-fit: contain\"/>\r\n        <p>创建时间：{{ detail.created_at }}</p>\r\n        <p>更新时间：{{ detail.updated_at }}</p>\r\n        <div v-html=\"detail.content\" style=\"margin-top: 10px\"/>\r\n      </div>\r\n    </el-dialog>\r\n\r\n    <!-- 编辑弹窗 -->\r\n    <el-dialog v-model=\"editDialogVisible\" @close=\"showEditEditor = false\" title=\"编辑发型\">\r\n    <el-form :model=\"editForm\" label-width=\"80px\">\r\n        <el-form-item label=\"名称\">\r\n          <el-input v-model=\"editForm.hairstyle_name\"/>\r\n        </el-form-item>\r\n        <el-form-item label=\"封面\">\r\n          <div style=\"display: flex; align-items: center; gap: 20px\">\r\n            <img v-if=\"editForm.hairstyle_pic\" :src=\"editForm.hairstyle_pic\" style=\"width: 200px; height: 120px\"/>\r\n            <el-upload :show-file-list=\"false\" :http-request=\"handleCustomUpload\" accept=\"image/*\">\r\n              <el-button type=\"primary\">上传封面</el-button>\r\n            </el-upload>\r\n          </div>\r\n        </el-form-item>\r\n      <el-form-item label=\"内容\">\r\n        <div v-if=\"showEditEditor\" class=\"editor-wrapper\">\r\n          <Toolbar v-if=\"showEditEditor\" :editor=\"editor1\" mode=\"default\" :key=\"editEditorKey\" />\r\n          <Editor\r\n              v-if=\"showEditEditor\"\r\n              v-model=\"editForm.content\"\r\n              :defaultConfig=\"editorConfig\"\r\n              mode=\"default\"\r\n              :key=\"editEditorKey\"\r\n              style=\"height: 300px; overflow-y: auto\"\r\n              @onCreated=\"editor => (editor1 = editor)\"\r\n              @onChange=\"handleEditorChange1\"\r\n          />\r\n\r\n        </div>\r\n      </el-form-item>\r\n\r\n\r\n    </el-form>\r\n      <template #footer>\r\n        <el-button @click=\"editDialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"handleSaveEdit\">保存</el-button>\r\n      </template>\r\n    </el-dialog>\r\n\r\n    <!-- 新建弹窗 -->\r\n    <el-dialog v-model=\"createDialogVisible\" @close=\"showCreateEditor = false\" title=\"新建发型\">\r\n    <el-form :model=\"createForm\" label-width=\"80px\">\r\n        <el-form-item label=\"名称\">\r\n          <el-input v-model=\"createForm.hairstyle_name\"/>\r\n        </el-form-item>\r\n        <el-form-item label=\"封面\">\r\n          <div style=\"display: flex; align-items: center; gap: 20px\">\r\n            <img v-if=\"createForm.hairstyle_pic\" :src=\"createForm.hairstyle_pic\" style=\"width: 200px; height: 120px\"/>\r\n            <el-upload :show-file-list=\"false\" :http-request=\"handleCreateUpload\" accept=\"image/*\">\r\n              <el-button type=\"primary\">上传封面</el-button>\r\n            </el-upload>\r\n          </div>\r\n        </el-form-item>\r\n      <el-form-item label=\"内容\">\r\n        <div v-if=\"showCreateEditor\" class=\"editor-wrapper\">\r\n          <Toolbar :editor=\"editor2\" mode=\"default\"/>\r\n          <Editor\r\n              v-model=\"createForm.content\"\r\n              :defaultConfig=\"editorConfig\"\r\n              mode=\"default\"\r\n              style=\"height: 300px; overflow-y: auto\"\r\n  