er = useRouter()\r\n\r\nconst walletInfo = ref({\r\n  coins: 0,\r\n  membership: '无'  // 可选值如：普通会员 / 高级会员 / 无\r\n})\r\n\r\nconst membershipMap = {\r\n  none: '无',\r\n  basic: '普通会员',\r\n  premium: '高级会员'\r\n}\r\n\r\nconst fetchWalletInfo = async () => {\r\n  try {\r\n    const res = await userWalletService()\r\n    walletInfo.value = res.data\r\n  } catch (err) {\r\n    ElMessage.error('钱包信息加载失败')\r\n  }\r\n}\r\n\r\nconst backToHome = () => {\r\n  router.push('/')\r\n}\r\n\r\nonMounted(() => {\r\n  fetchWalletInfo()\r\n})\r\n</script>\r\n\r\n<template>\r\n  <el-container class=\"view-box\">\r\n    <el-main class=\"main\">\r\n      <div class=\"main-content\">\r\n\r\n        <!-- 返回首页按钮 -->\r\n        <el-button\r\n            class=\"back-btn\"\r\n            type=\"primary\"\r\n            icon=\"el-icon-arrow-left\"\r\n            @click=\"backToHome\"\r\n            plain\r\n        >\r\n          返回首页\r\n        </el-button>\r\n\r\n        <div class=\"wallet-box\">\r\n          <div class=\"wallet-icon\">\r\n            <img src=\"@/assets/wallet.png\" alt=\"钱包图标\" />\r\n          </div>\r\n          <div class=\"info\">\r\n            <el-descriptions title=\"我的钱包\" column=\"1\" border>\r\n              <el-descriptions-item label=\"金币余额\">\r\n                <span style=\"font-weight: bold; font-size: 22px;\">{{ walletInfo.coins }} 枚</span>\r\n              </el-descriptions-item>\r\n              <el-descriptions-item label=\"会员状态\">\r\n                <el-tag :type=\"walletInfo.membership === 'premium' ? 'success' : walletInfo.membership === 'basic' ? 'info' : 'default'\">\r\n                  {{ membershipMap[walletInfo.membership] || '无' }}\r\n                </el-tag>\r\n              </el-descriptions-item>\r\n            </el-descriptions>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </el-main>\r\n  </el-container>\r\n</template>\r\n\r\n<style scoped>\r\n.main-content {\r\n  flex: 1;\r\n  width: 100%;\r\n  max-width: 800px;\r\n  margin: