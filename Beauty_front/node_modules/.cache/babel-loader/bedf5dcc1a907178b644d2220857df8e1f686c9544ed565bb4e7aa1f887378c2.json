 = ref(0)\r\nconst pageNum = ref(1)\r\nconst pageSize = ref(12)  // 每页8个卡片\r\n\r\nconst getCreations = async () => {\r\n  try {\r\n    const res = await creationList({\r\n      pageNum: pageNum.value,\r\n      pageSize: pageSize.value\r\n    })\r\n    creations.value = res.data.items\r\n    total.value = res.data.total\r\n  } catch (err) {\r\n    ElMessage.error('数据加载失败')\r\n  }\r\n}\r\n\r\nonMounted(() => {\r\n  getCreations()\r\n})\r\n\r\nconst handleCurrentChange = (val) => {\r\n  pageNum.value = val\r\n  getCreations()\r\n}\r\n</script>\r\n\r\n<template>\r\n  <el-container class=\"view-box\">\r\n    <el-main class=\"main\">\r\n      <div class=\"main-content\">\r\n\r\n        <el-card class=\"card\">\r\n          <template #header>\r\n            <div class=\"card-header\">\r\n              <span>内容展示</span>\r\n            </div>\r\n          </template>\r\n\r\n          <div class=\"card-grid\">\r\n            <CardCreation\r\n                style=\"margin-right: 20px; margin-left: 20px\"\r\n                v-for=\"item in creations\"\r\n                :key=\"item.id\"\r\n                :data=\"item\"\r\n                :loading=\"false\"\r\n            />\r\n          </div>\r\n\r\n          <el-pagination\r\n              layout=\"prev, pager, next\"\r\n              :total=\"total\"\r\n              :page-size=\"pageSize\"\r\n              :current-page=\"pageNum\"\r\n              @current-change=\"handleCurrentChange\"\r\n              background\r\n              style=\"margin-top: 20px; text-align: center;\"\r\n          />\r\n        </el-card>\r\n\r\n        <el-card class=\"card\">\r\n          <div style=\"margin-top: 20px; display: flex; height: 120px\">\r\n            <p style=\"font-size: 20px; font-family: 'Segoe Script'; margin-top: 100px;\">Beauty</p>\r\n            <p style=\"font-size: 20px; margin-left: 5px; margin-top: 104px;\">Created by Xiaoshashashashasha</p>\r\n          </div>\r\n        </el-card>\r\n      </div>\r\n    </el-main>\