{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { ElMessage } from 'element-plus';\nimport { myReservationList } from '@/api/reservation';\nexport default {\n  __name: 'MyReservation',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const router = useRouter();\n    const tableData = ref([]);\n    const pageNum = ref(1);\n    const pageSize = ref(15);\n    const total = ref(0);\n    const status = ref(0);\n    const statusMap = {\n      0: '尚未履约',\n      1: '已履约未评价',\n      2: '爽约',\n      3: '已评价'\n    };\n    const fetchReservations = async () => {\n      try {\n        const res = await myReservationList({\n          pageNum: pageNum.value,\n          pageSize: pageSize.value,\n          status: status.value\n        });\n        tableData.value = res.data.items || [];\n        total.value = res.data.total || 0;\n      } catch (err) {\n        ElMessage.error(err.msg || '获取预约信息失败');\n      }\n    };\n    const handlePageChange = newPage => {\n      pageNum.value = newPage;\n      fetchReservations();\n    };\n    const handleStatusChange = newStatus => {\n      status.value = newStatus;\n      pageNum.value = 1;\n      fetchReservations();\n    };\n    onMounted(() => {\n      fetchReservations();\n    });\n    const __returned__ = {\n      router,\n      tableData,\n      pageNum,\n      pageSize,\n      total,\n      status,\n      statusMap,\n      fetchReservations,\n      handlePageChange,\n      handleStatusChange,\n      ref,\n      onMounted,\n      get useRouter() {\n        return useRouter;\n      },\n      get ElMessage() {\n        return ElMessage;\n      },\n      get myReservationList() {\n        return myReservationList;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","useRouter","ElMessage","myReservationList","router","tableData","pageNum","pageSize","total","status","statusMap","fetchReservations","res","value","data","items","err","error","msg","handlePageChange","newPage","handleStatusChange","newStatus"],"sources":["C:/Users/32561/Desktop/毕设仓库/beauty-front/src/views/Mine/MyReservation.vue"],"sourcesContent":["<script setup>\r\nimport { ref, onMounted } from 'vue'\r\nimport { useRouter } from 'vue-router'\r\nimport { ElMessage } from 'element-plus'\r\nimport { myReservationList } from '@/api/reservation'\r\n\r\nconst router = useRouter()\r\nconst tableData = ref([])\r\nconst pageNum = ref(1)\r\nconst pageSize = ref(15)\r\nconst total = ref(0)\r\nconst status = ref(0)\r\n\r\nconst statusMap = {\r\n  0: '尚未履约',\r\n  1: '已履约未评价',\r\n  2: '爽约',\r\n  3: '已评价'\r\n}\r\n\r\nconst fetchReservations = async () => {\r\n  try {\r\n    const res = await myReservationList({ pageNum: pageNum.value, pageSize: pageSize.value, status: status.value })\r\n    tableData.value = res.data.items || []\r\n    total.value = res.data.total || 0\r\n  } catch (err) {\r\n    ElMessage.error(err.msg || '获取预约信息失败')\r\n  }\r\n}\r\n\r\nconst handlePageChange = (newPage) => {\r\n  pageNum.value = newPage\r\n  fetchReservations()\r\n}\r\n\r\nconst handleStatusChange = (newStatus) => {\r\n  status.value = newStatus\r\n  pageNum.value = 1\r\n  fetchReservations()\r\n}\r\n\r\nonMounted(() => {\r\n  fetchReservations()\r\n})\r\n</script>\r\n\r\n<template>\r\n  <div class=\"main-content\">\r\n    <el-button class=\"back-btn\" type=\"primary\" plain @click=\"() => router.push('/')\">\r\n      返回首页\r\n    </el-button>\r\n\r\n    <div class=\"section-title\">我的预约</div>\r\n\r\n    <div style=\"margin-bottom: 20px;\">\r\n      <el-select v-model=\"status\" placeholder=\"请选择预约状态\" @change=\"handleStatusChange\" style=\"width: 200px;\">\r\n        <el-option :value=\"0\" label=\"尚未履约\" />\r\n        <el-option :value=\"1\" label=\"已履约未评价\" />\r\n        <el-option :value=\"2\" label=\"爽约\" />\r\n        <el-option :value=\"3\" label=\"已评价\" />\r\n      </el-select>\r\n    </div>\r\n\r\n    <el-table :data=\"tableData\" style=\"width: 100%;\">\r\n      <el-table-column prop=\"reservation_id\" label=\"预约ID\" width=\"100\" />\r\n      <el-table-column prop=\"offline_name\" label=\"门店名称\" width=\"160\" />\r\n      <el-table-column prop=\"member_name\" label=\"预约成员\" width=\"140\" />\r\n      <el-table-column prop=\"start_at\" label=\"预约开始时间\" width=\"180\" />\r\n      <el-table-column prop=\"end_at\" label=\"预约结束时间\" width=\"180\" />\r\n      <el-table-column prop=\"status\" label=\"状态\" width=\"120\">\r\n        <template #default=\"{ row }\">\r\n          <el-tag>{{ statusMap[row.status] }}</el-tag>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"comment\" label=\"评论\" min-width=\"800\">\r\n        <template #default=\"{ row }\">\r\n          <span v-if=\"row.comment\">{{ row.comment }}</span>\r\n          <span v-else style=\"color: #999;\">暂无评论</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"220\">\r\n        <template #default=\"{ row }\">\r\n          <template v-if=\"row.status === 0\">\r\n            <el-button size=\"small\" type=\"danger\">取消预约</el-button>\r\n          </template>\r\n          <template v-else-if=\"row.status === 1\">\r\n            <el-button size=\"small\" type=\"primary\">去评价</el-button>\r\n          </template>\r\n          <template v-else>\r\n            <span style=\"color: #999;\">无可操作项</span>\r\n          </template>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <div style=\"text-align: center; margin-top: 20px;\">\r\n      <el-pagination\r\n          background\r\n          layout=\"prev, pager, next, ->, total\"\r\n          :total=\"total\"\r\n          :current-page=\"pageNum\"\r\n          :page-size=\"pageSize\"\r\n          @current-change=\"handlePageChange\"\r\n      />\r\n    </div>\r\n  </div>\r\n