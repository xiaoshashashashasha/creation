{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport CardCreation from \"@/components/CardCreation.vue\";\nimport CardHairstyle from \"@/components/CardHairstyle.vue\";\nimport CardOffline from \"@/components/CardOffline.vue\";\nimport { onMounted, ref } from \"vue\";\nimport { ElMessage } from \"element-plus\";\nimport { creationList, creationInfo } from \"@/api/creation\";\nimport router from \"@/router\";\n\n/**\r\n * creation 内容数据\r\n */\n\nexport default {\n  __name: 'HomePage',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const creationData = ref([]);\n    const fetchCreationList = async () => {\n      try {\n        const res = await creationList({\n          pageNum: 1,\n          pageSize: 8\n        }); // 前端分页\n        creationData.value = res.data.items; // 后端返回 PageBean\n      } catch (e) {\n        ElMessage.error('加载内容失败');\n      }\n    };\n\n    /**\r\n     * 轮播图数据\r\n     */\n    const carouselData = ref([]);\n    const fetchCarouselData = async () => {\n      const ids = [3, 4, 5, 6];\n      const data = [];\n      for (const id of ids) {\n        const res = await creationInfo(id);\n        data.push({\n          id: id,\n          image: res.data.cover_pic,\n          title: res.data.title\n        });\n      }\n      carouselData.value = data;\n    };\n    const toContent = id => {\n      router.push(`/content/creation/${id}`);\n    };\n    onMounted(() => {\n      fetchCreationList();\n      fetchCarouselData();\n    });\n    const __returned__ = {\n      creationData,\n      fetchCreationList,\n      carouselData,\n      fetchCarouselData,\n      toContent,\n      CardCreation,\n      CardHairstyle,\n      CardOffline,\n      onMounted,\n      ref,\n      get ElMessage() {\n        return ElMessage;\n      },\n      get creationList() {\n        return creationList;\n      },\n      get creationInfo() {\n        return creationInfo;\n      },\n      get router() {\n        return router;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["CardCreation","CardHairstyle","CardOffline","onMounted","ref","ElMessage","creationList","creationInfo","router","creationData","fetchCreationList","res","pageNum","pageSize","value","data","items","e","error","carouselData","fetchCarouselData","ids","id","push","image","cover_pic","title","toContent"],"sources":["C:/Users/32561/Desktop/毕设仓库/beauty-front/src/views/HomePage.vue"],"sourcesContent":["<script setup>\r\nimport CardCreation from \"@/components/CardCreation.vue\";\r\nimport CardHairstyle from \"@/components/CardHairstyle.vue\";\r\nimport CardOffline from \"@/components/CardOffline.vue\";\r\nimport {onMounted, ref} from \"vue\";\r\nimport {ElMessage} from \"element-plus\";\r\nimport {creationList, creationInfo} from \"@/api/creation\";\r\nimport router from \"@/router\";\r\n\r\n/**\r\n * creation 内容数据\r\n */\r\nconst creationData = ref([])\r\n\r\nconst fetchCreationList = async () => {\r\n  try {\r\n    const res = await creationList({pageNum: 1, pageSize: 8})  // 前端分页\r\n    creationData.value = res.data.items  // 后端返回 PageBean\r\n  } catch (e) {\r\n    ElMessage.error('加载内容失败')\r\n  }\r\n}\r\n\r\n/**\r\n * 轮播图数据\r\n */\r\nconst carouselData = ref([])\r\n\r\nconst fetchCarouselData = async () => {\r\n  const ids = [3, 4, 5, 6]\r\n  const data = []\r\n  for (const id of ids) {\r\n    const res = await creationInfo(id)\r\n    data.push({\r\n      id: id,\r\n      image: res.data.cover_pic,\r\n      title: res.data.title\r\n    })\r\n  }\r\n  carouselData.value = data\r\n}\r\n\r\nconst toContent = (id) => {\r\n  router.push(`/content/creation/${id}`)\r\n}\r\n\r\nonMounted(() => {\r\n  fetchCreationList()\r\n  fetchCarouselData()\r\n})\r\n</script>\r\n\r\n<template>\r\n  <el-container class=\"view-box\">\r\n    <el-main class=\"main\">\r\n      <div class=\"main-content\">\r\n        <!-- 轮播图 -->\r\n        <el-carousel :interval=\"4000\" type=\"card\" class=\"cardpic\" card-scale=\"0.7\">\r\n          <el-carousel-item\r\n              v-for=\"item in carouselData\"\r\n              :key=\"item.id\"\r\n              @click=\"toContent(item.id)\">\r\n            <img :src=\"item.image\" alt=\"轮播图\" class=\"carousel-item\"/>\r\n            <div class=\"carousel-title\">{{ item.title }}</div>\r\n          </el-carousel-item>\r\n        </el-carousel>\r\n\r\n        <!-- 内容卡片 -->\r\n        <el-card class=\"card\">\r\n          <template #header>\r\n            <div class=\"card-header\">\r\n              <span>内容</span>\r\n              <span>更多</span>\r\n            </div>\r\n          </template>\r\n          <div style=\"margin-top: 20px; display: flex; flex-wrap: wrap; justify-content: center;\">\r\n            <CardCreation\r\n                v-for=\"item in creationData\"\r\n                :key=\"item.id\"\r\n                :creation=\"item\"\r\n                :loading=\"false\"\r\n                class=\"card-creation\"\r\n            />\r\n          </div>\r\n        </el-card>\r\n\r\n        <!-- 其他模块（不动） -->\r\n        <el-card class=\"card\">\r\n          <div style=\"margin-top: 20px; display: flex; height: 120px\">\r\n            <p style=\"font-size: 20px; font-family: 'Segoe Script'; margin-top: 100px;\">Beayty</p>\r\n            <p style=\"font-size: 20px; margin-left: 5px; margin-top: 104px;\">Created by Xiaoshashashashasha</p>\r\n          </div>\r\n        </el-card>\r\n\r\n      </div>\r\n    </el-main>\r\n  </el-container>\r\n</template>\r\n"],"mappings":";AACA,OAAOA,YAAY,MAAM,+BAA+B;AACxD,OAAOC,aAAa,MAAM,gCAAgC;AAC1D,OAAOC,WAAW,MAAM,8BAA8B;AACtD,SAAQC,SAAS,EAAEC,GAAG,QAAO,KAAK;AAClC,SAAQC,SAAS,QAAO,cAAc;AACtC,SAAQC,YAAY,EAAEC,YAAY,QAAO,gBAAgB;AACzD,OAAOC,MAAM,MAAM,UAAU;;AAE7B;AACA;AACA;;;;;;;;IACA,MAAMC,YAAY,GAAGL,GAAG,CAAC,EAAE,CAAC;IAE5B,MAAMM,iBAAiB,GAAG,MAAAA,CAAA,KAAY;MACpC,IAAI;QACF,MAAMC,GAAG,GAAG,MAAML,Y