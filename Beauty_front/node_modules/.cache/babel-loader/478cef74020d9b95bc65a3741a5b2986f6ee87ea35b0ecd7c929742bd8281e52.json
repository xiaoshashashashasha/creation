{"ast":null,"code":"import { ref, reactive, onMounted } from \"vue\";\nimport { ElMessage } from \"element-plus\";\nimport { offlineRequestListExa, offlineExamine } from \"@/api/offline\";\nimport { userListService } from \"@/api/user\";\nexport default {\n  __name: 'RequestManage',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const tableData = ref([]);\n    const pageNum = ref(1);\n    const pageSize = ref(10);\n    const total = ref(0);\n    const reasonMap = reactive({}); // 存储每一行否决原因\n    const userInfoMap = reactive({}); // 存储每个 manager_id 的用户信息\n\n    // 获取店长的昵称与邮箱\n    const fetchManagerInfo = async managerId => {\n      if (userInfoMap[managerId]) return; // 已缓存\n\n      try {\n        const res = await userListService({\n          user_id: managerId\n        });\n        const user = res.data.items?.[0] || {};\n        userInfoMap[managerId] = {\n          nickname: user.nickname || \"-\",\n          email: user.email || \"-\"\n        };\n      } catch (err) {\n        userInfoMap[managerId] = {\n          nickname: \"-\",\n          email: \"-\"\n        };\n        console.warn(\"获取用户失败\", err);\n      }\n    };\n\n    // 加载审核列表\n    const fetchData = async () => {\n      try {\n        const res = await offlineRequestListExa({\n          pageNum: pageNum.value,\n          pageSize: pageSize.value\n        });\n        tableData.value = res.data.items || [];\n        total.value = res.data.total || 0;\n\n        // 加载对应的用户信息\n        for (const item of tableData.value) {\n          await fetchManagerInfo(item.manager_id);\n        }\n      } catch (err) {\n        ElMessage.error(err.msg || \"获取审核列表失败\");\n      }\n    };\n    const handlePageChange = newPage => {\n      pageNum.value = newPage;\n      fetchData();\n    };\n\n    // 审核操作\n    const handleExamine = async (row, pass) => {\n      const reason = reasonMap[row.request_id] || \"\";\n      if (!pass && !reason.trim()) {\n        return ElMessage.warning(\"请填写否决原因\");\n      }\n      try {\n        await offlineExamine(row.request_id, pass ? 0 : 1, reason);\n        ElMessage.success(pass ? \"已通过\" : \"已否决\");\n        fetchData();\n      } catch (err) {\n        ElMessage.error(err.msg || \"操作失败\");\n      }\n    };\n    onMounted(() => {\n      fetchData();\n    });\n    const __returned__ = {\n      tableData,\n      pageNum,\n      pageSize,\n      total,\n      reasonMap,\n      userInfoMap,\n      fetchManagerInfo,\n      fetchData,\n      handlePageChange,\n      handleExamine,\n      ref,\n      reactive,\n      onMounted,\n      get ElMessage() {\n        return ElMessage;\n      },\n      get offlineRequestListExa() {\n        return offlineRequestListExa;\n      },\n      get offlineExamine() {\n        return offlineExamine;\n      },\n      get userListService() {\n        return userListService;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","reactive","onMounted","ElMessage","offlineRequestListExa","offlineExamine","userListService","tableData","pageNum","pageSize","total","reasonMap","userInfoMap","fetchManagerInfo","managerId","res","user_id","user","data","items","nickname","email","err","console","warn","fetchData","value","item","manager_id","error","msg","handlePageChange","newPage","handleExamine","row","pass","reason","request_id","trim","warning","success"],"sources":["C:/Users/32561/Desktop/毕设仓库/beauty-front/src/views/manage/offline/RequestManage.vue"],"sourcesContent":["<script setup>\r\nimport { ref, reactive, onMounted } from \"vue\"\r\nimport { ElMessage } from \"element-plus\"\r\nimport { offlineRequestListExa, offlineExamine} from \"@/api/offline\"\r\nimport { userListService} from \"@/api/user\"\r\n\r\nconst tableData = ref([])\r\nconst pageNum = ref(1)\r\nconst pageSize = ref(10)\r\nconst total = ref(0)\r\nconst reasonMap = reactive({})  // 存储每一行否决原因\r\nconst userInfoMap = reactive({}) // 存储每个 manager_id 的用户信息\r\n\r\n// 获取店长的昵称与邮箱\r\nconst fetchManagerInfo = async (managerId) => {\r\n  if (userInfoMap[managerId]) return // 已缓存\r\n\r\n  try {\r\n    const res = await userListService({ user_id: managerId })\r\n    const user = res.data.items?.[0] || {}\r\n    userInfoMap[managerId] = {\r\n      nickname: user.nickname || \"-\",\r\n      email: user.email || \"-\"\r\n    }\r\n  } catch (err) {\r\n    userInfoMap[managerId] = {\r\n      nickname: \"-\",\r\n      email: \"-\"\r\n    }\r\n    console.warn(\"获取用户失败\", err)\r\n  }\r\n}\r\n\r\n// 加载审核列表\r\nconst fetchData = async () => {\r\n  try {\r\n    const res = await offlineRequestListExa({\r\n      pageNum: pageNum.value,\r\n      pageSize: pageSize.value\r\n    })\r\n    tableData.value = res.data.items || []\r\n    total.value = res.data.total || 0\r\n\r\n    // 加载对应的用户信息\r\n    for (const item of tableData.value) {\r\n      await fetchManagerInfo(item.manager_id)\r\n    }\r\n  } catch (err) {\r\n    ElMessage.error(err.msg || \"获取审核列表失败\")\r\n  }\r\n}\r\n\r\nconst handlePageChange = (newPage) => {\r\n  pageNum.value = newPage\r\n  fetchData()\r\n}\r\n\r\n// 审核操作\r\nconst handleExamine = async (row, pass) => {\r\n  const reason = reasonMap[row.request_id] || \"\"\r\n  if (!pass && !reason.trim()) {\r\n    return ElMessage.warning(\"请填写否决原因\")\r\n  }\r\n  try {\r\n    await offlineExamine(row.request_id, pass ? 0 : 1, reason)\r\n    ElMessage.success(pass ? \"已通过\" : \"已否决\")\r\n    fetchData()\r\n  } catch (err) {\r\n    ElMessage.error(err.msg || \"操作失败\")\r\n  }\r\n}\r\n\r\nonMounted(() => {\r\n  fetchData()\r\n})\r\n</script>\r\n\r\n\r\n<template>\r\n  <el-row :gutter=\"0\">\r\n    <div class=\"path\">\r\n      <p style=\"font-size: 20px; margin-top: 5px\">门店审核</p>\r\n    </div>\r\n\r\n    <div class=\"search\">\r\n\r\n    </div>\r\n\r\n    <el-table :data=\"tableData\" style=\"width: 100%; margin-top: 10px\">\r\n      <el-table-column prop=\"request_id\" label=\"申请ID\" width=\"120\"/>\r\n      <el-table-column prop=\"manager_id\" label=\"店长ID\" width=\"120\"/>\r\n      <el-table-column prop=\"nickname\" label=\"店长昵称\" width=\"160\"/>\r\n      <el-table-column prop=\"email\" label=\"店长邮箱\" width=\"240\"/>\r\n      <el-table-column prop=\"target_city\" label=\"目标城市\" width=\"160\"/>\r\n      <el-table-column label=\"否决原因\" width=\"240\">\r\n        <template #default=\"{ row }\">\r\n          {{ row.review_comments || '-' }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"240\">\r\n        <template #default=\"{ row }\">\r\n          <el-button type=\"success\" size=\"small\" @click=\"handleApprove(row.request_id)\">通过</el-button>\r\n          <el-button type=\"danger\" size=\"small\" @click=\"handleReject(row.request_id)\">否决</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <div style=\"text-align: center; margin-top: 20px\">\r\n      <el-pagination\r\n          background\r\n          layout=\"prev, pager, next, ->, total\"\r\n          :total=\"total\"\r\n          :current-page=\"pageNum\"\r\n          :page-size=\"pageSize\"\r\n          @current-change=\"handlePageChange\"\r\n      />\r\n    </div>\r\n  </el-row>\r\n</template>\r\n\r\n<style scoped>\r\n.path {\r\n  height: 40px;\r\n  width: 100%;\r\n  background-color: #d8a6a6;\r\n}\r\n\r\n::v-deep(.el-table__row) {\r\n  height: 60px;\r\n}\r\n\r\n.search {\r\n  width: 100%;\r\n  height: 52px;\r\n  background-color: #bbd2e3;\r\n}\r\n</style>\r\n"],"mappings":"AACA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,SAAS,QAAQ,KAAK;AAC9C,SAASC,SAAS,QAAQ,cAAc;AACxC,SAASC,qBAAqB,EAAEC,cAAc,QAAO,eAAe;AACpE,SAASC,eAAe,QAAO,YAAY;;;;;;;IAE3C,MAAMC,SAAS,GAAGP,GAAG,CAAC,EAAE,CAAC;IACzB,MAAMQ,OAAO,GAAGR,GAAG,CAAC,CAAC,CAAC;IACtB,MAAMS,QAAQ,GAAGT,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMU,KAAK,GAAGV,GAAG,CAAC,CAAC,CAAC;IACpB,MAAMW,SAAS,GAAGV,QAAQ,CAAC,CAAC,CAAC,CAAC,EAAE;IAChC,MAAMW,WAAW,GAAGX,QAAQ,CAAC,CAAC,CAAC