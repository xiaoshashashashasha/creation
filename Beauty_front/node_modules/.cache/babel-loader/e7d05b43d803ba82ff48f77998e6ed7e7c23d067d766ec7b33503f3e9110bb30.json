tartsWith('/manage'))\r\nconst router = useRouter()\r\nconst tokenStore = useTokenStore()\r\nconst stateStore = useStateStore()\r\nconst mine = computed(() => [0, 1, 2].includes(stateStore.sta))\r\n\r\nconst isOpen = ref(false)\r\n\r\nconst userInfo = ref({})\r\nconst follows = ref([])\r\nconst fans = ref([])\r\n\r\nconst goToMyInfo = () => {\r\n  router.push('/myInfo')\r\n  isOpen.value = false\r\n}\r\n\r\nconst goToMyWallet = ()=>{\r\n  router.push('/myWallet')\r\n  isOpen.value = false\r\n}\r\n\r\nconst goToMyCreation = ()=>{\r\n  router.push('/myCreation')\r\n  isOpen.value = false\r\n}\r\n\r\nconst goToMyCollection = ()=>{\r\n  router.push('/myFavorites')\r\n  isOpen.value = false\r\n}\r\n\r\nconst goToMyRequest = ()=>{\r\n  router.push('/myRequest')\r\n  isOpen.value = false\r\n}\r\nconst goToMyOffline = ()=>{\r\n  router.push('/myOffline')\r\n  isOpen.value = false\r\n}\r\n\r\nconst goToOtherInfo = (user_id) => {\r\n  router.push(`/otherInfo/${user_id}`)\r\n  isOpen.value = false\r\n}\r\n\r\nconst goToMyMessage = ()=>{\r\n  router.push('/myMessage')\r\n}\r\n\r\nconst goToMyReservation = ()=>{\r\n  router.push('/myReservation')\r\n  isOpen.value = false\r\n}\r\n\r\n\r\nconst toggleSidebar = async () => {\r\n  isOpen.value = !isOpen.value\r\n  if (isOpen.value) {\r\n    await fetchUserData()\r\n  }\r\n}\r\n\r\n\r\nconst fetchUserData = async () => {\r\n  try {\r\n    const res = await userInfoService()\r\n    userInfo.value = res.data\r\n  } catch (err) {\r\n    if (err?.response?.status === 401) {\r\n      // 静默处理，不弹出消息\r\n      console.warn('[Sidebar] 未登录，跳过用户信息拉取')\r\n    } else {\r\n      console.error(err)\r\n      ElMessage.error(err.msg || '用户信息加载失败')\r\n    }\r\n  }\r\n}\r\n\r\nconst fetchFollows = async () => {\r\n  const res = await listFollowedService()\r\n  follows.value = res.data || []\r\n}\r\n\r\nconst fetchFans = async () => {\r\n  const res = await listFollowerService()\r\n  fans.value = res.data || []\r\n}\r\n\r\nonMounted(() => {\r\n  if (!to